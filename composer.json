{
    "name": "web-illinois/illinois_framework_core",
    "description": "Contrib and custom modules that are required by the Illinois Framework",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "require": {
        "drupal-ckeditor-libraries-group/fakeobjects": "^4.17",
        "drupal-ckeditor-libraries-group/link": "^4.17",
        "drupal/admin_toolbar": "^3.0",
        "drupal/advanced_text_formatter": "^3.0@RC",
        "drupal/anchor_link": "^2.4",
        "drupal/backup_migrate": "^5.0",
        "drupal/bamboo_twig": "^6.0",
        "drupal/block_exclude_pages": "^2.1@beta",
        "drupal/colorbox": "^2.0",
        "drupal/config_sync": "^3.0@alpha",
        "drupal/diff": "^1.0",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/extlink": "^1.3",
        "drupal/google_analytics": "^4.0",
        "drupal/google_cse": "^3.1",
        "drupal/google_tag": "^1.3",
        "drupal/image_widget_crop": "^2.3",
        "drupal/imagick": "^1.5",
        "drupal/linkit": "^6.0",
        "drupal/linkit_media_library": "^1.0",
        "drupal/media_remote": "^1.2",
        "drupal/menu_block": "^1.6",
        "drupal/metatag": "^1.13",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/pathologic": "^2.0@alpha",
        "drupal/r4032login": "^2.1",
        "drupal/redirect": "^1.6",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/video_embed_field": "^2.4",
        "npm-asset/blazy": "~1.0",
        "npm-asset/slick-carousel": "~1.0",
        "bower-asset/colorbox": "^1.6"
    },
    "config": {
        "lock": false,
        "preferred-install": {
            "drupal/core": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "file-mapping": {
                "[project-root]/.gitattributes": false,
                "[project-root]/.editorconfig": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/.csslintrc": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/robots.txt": false,
                "[web-root]/web.config": false
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "drupal-libraries": {
            "library-directory": "docroot/libraries"
          },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
